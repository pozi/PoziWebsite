{
  "dependencies": {
    "retypeapp": "2.4.0"
  },
  "devDependencies": {
    "browser-sync": "2.27.9",
    "concurrently": "7.1.0"
  },
  "scripts": {
    "retype-watch-no-browser": "DISPLAY=:none retype watch",
    "browser-sync": "browser-sync start --no-open --files \".\" --ignore \"node_modules\" \".git\" --port 3010 --proxy http://localhost:5000/ --reload-delay 1500",
    "start": "concurrently \"npm:retype-watch-no-browser\" \"npm:browser-sync\""
  }
}
